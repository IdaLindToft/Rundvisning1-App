            <RadioButton Grid.Column="1"></RadioButton>
            <RadioButton Grid.Column="2"></RadioButton>
            <RadioButton Grid.Column="3"></RadioButton>
        </Grid>

        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"></ColumnDefinition>
                <ColumnDefinition Width="4*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBlock Grid.Column="0" Text="Andet:" ></TextBlock>
            <TextBox TextWrapping="WrapWholeWords" MinHeight="20" MaxHeight="150" Grid.Column="1"></TextBox>
        </Grid>
        <Button Margin="0 10 0 0" HorizontalAlignment="Center" Content="Send"></Button>
    </StackPanel>
</Page>
